@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser>
@{
    ViewBag.Title = "Admin";
}

@foreach (var user in Model)
{
    <table class="table">
        <tr>
            <td>
                @Html.DisplayFor(modelItem => user.Email)
            </td>
            <td>
                <a asp-action="AdminDetails" asp-controller="Home" asp-route-id="@user.Id">Open</a>
            </td>
        </tr>
    </table>
}
